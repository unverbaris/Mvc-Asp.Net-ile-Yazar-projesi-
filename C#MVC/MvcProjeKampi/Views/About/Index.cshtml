@using EntityLayer.Concrete
@model List<About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkında veri listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Hakkında Başlığı</th>
        <th>Aktif</th>
        <th>Pasif</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutID</td>
            <td>Başlık</td>
            <td><a href="#" class="btn btn-warning">Aktif yap</a></td>
            <td><a href="#" class="btn btn-danger">Pasif Yap</a></td>
        </tr>

    }



</table>



@Html.Action("AboutPartial","About")

